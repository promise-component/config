{
	"name": "promise/component-config",
	"description": "Manage Promise configuration by persistent storage.",
	"license": "MIT",
	"homepage": "https://baersi.app",
	"authors": [
		{
			"name": "Bill li",
			"email": "bill.li@ofcold.com"
		}
	],
	"require": {
		"php": "^8.0|^7.4",
		"promise/component-support": "dev-master",
		"illuminate/support": "^8.0"
	},
	"require-dev": {
		"mockery/mockery": "^1.3.1",
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"Promise\\Component\\Config\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Promise\\Component\\ConfigTests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev"
}
